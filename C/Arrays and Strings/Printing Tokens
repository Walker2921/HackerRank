#include<stdio.h>
#include<stdlib.h>
int main()
{
    int len=0,i;
    char a[1000];
    gets(a);
    for(i=0;i<1000;i++)
    {
        if(a[i]==NULL)   
            break;
        len++;    
    }
    for(i=0;i<len;i++)
    {
        if(a[i]==' ')
            printf("\n");
        else
            printf("%c",a[i]);
    }
    return 0;
}
